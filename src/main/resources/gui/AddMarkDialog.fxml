<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.lang.Double?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.BorderPane?>


<VBox prefWidth="327.0"  xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.soundlooper.system.AddMarkController">
   <children>
      <AnchorPane prefHeight="26.0" minHeight="26" maxHeight="26" >
         <children>
            <ComboBox fx:id="comboBoxName" editable="true" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
	        </ComboBox>
         </children>
      </AnchorPane>
   </children>
   <children>
      <AnchorPane prefHeight="26.0" minHeight="26" maxHeight="26">
         <children>
         	<BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         		<center>
            		<Label fx:id="labelResultat" maxWidth="Infinity" maxHeight="Infinity"  focusTraversable="false" text="Label" style="-fx-alignment: CENTER_RIGHT">
            		   <styleClass>
		                  <String fx:value="bluetext" />
		               </styleClass>
		               <padding>
		               	  <Insets bottom="0" left=".0" right="10.0" top="0"/>
		               </padding>
		            </Label>
            	</center>
            	<right>
	            	<Button focusTraversable="false" maxHeight="Infinity" mnemonicParsing="false" onAction="#validate">
		               <styleClass>
		                  <String fx:value="validateButton" />
		               </styleClass>
		            </Button>
	            </right>
             </BorderPane>
         </children>
      </AnchorPane>
   </children>
   
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
  
</VBox>
